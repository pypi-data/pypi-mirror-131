			},
			{
				"name": "{name}_ch6",
				"get": "(({class_name}.{name}.Touched() & 0x040) ? 1.0f : 0.f)",
				"where": "main"
			},
			{
				"name": "{name}_ch7",
				"get": "(({class_name}.{name}.Touched() & 0x080) ? 1.0f : 0.f)",
				"where": "main"
			},
			{
				"name": "{name}_ch8",
				"get": "(({class_name}.{name}.Touched() & 0x100) ? 1.0f : 0.f)",
				"where": "main"
			},
			{
				"name": "{name}_ch9",
				"get": "(({class_name}.{name}.Touched() & 0x200) ? 1.0f : 0.f)",
				"where": "main"
			},
			{
				"name": "{name}_ch10",
