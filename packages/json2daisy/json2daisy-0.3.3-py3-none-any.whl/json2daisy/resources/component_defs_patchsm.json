		"periph": "daisy::I2CHandle::Config::Peripheral::I2C_1",
		"speed": "daisy::I2CHandle::Config::Speed::I2C_400KHZ",
		"mapping": [
			{
				"name": "{name}",
				"get": "{class_name}.{name}.GetAmount()",
