    <Description>
    Get the current calibration time.
    </Description>
    <Observable>No</Observable>
    <Parameter>
        <Identifier>Time</Identifier>
        <DisplayName>Time</DisplayName>
        <Description>
        Get the current calibration time.
        </Description>
        <DataType>
            <Constrained>
                <DataType>
                    <Basic>Integer</Basic>
                </DataType>
                <Constraints>
                </Constraints>
            </Constrained>
        </DataType>
    </Parameter>
    <Response>
        <Identifier>CurrentCalibrationTime</Identifier>
        <DisplayName>Current Calibration Time</DisplayName>
        <Description>Current Calibration Time.</Description>
        <DataType>
            <Basic>Integer</Basic>
        </DataType>
    </Response>
</Command>
<Command>
    <Identifier>GetRunTimeCalibration</Identifier>
    <DisplayName>Get Run Time since last Calibration</DisplayName>
    <Description>
    Get channel run time since last calibration.
    </Description>
    <Observable>No</Observable>
