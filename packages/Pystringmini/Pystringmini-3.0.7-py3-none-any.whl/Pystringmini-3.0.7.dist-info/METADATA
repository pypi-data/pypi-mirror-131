#Thiyagarajan V
from pystringmini import *
string="Hello"
print(uppercase(string))
print(lowercase(string))
print(capitalize(string))
print(length(string))
print(digit(string))
print(min(string))
print(max(string))
print(replace(string,'Hai','Everyone'))
print(encodeing(string))
print(decoding(string))
print(split(string))
print(help())


