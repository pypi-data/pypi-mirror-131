#!python
# -*- coding: utf-8 -*-
import sys
import time
import pathlib
import subprocess

if __name__ == "__main__":
    proc = subprocess.Popen(
        str(pathlib.Path(__file__).parent.absolute()) + "/nvdsw", shell=True
    )
    time.sleep(5)
    rc = proc.poll()
    if rc is None:
        sys.exit(0)
    else:
        sys.exit(rc)
